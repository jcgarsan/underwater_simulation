<?xml version="1.0"?>

<!DOCTYPE UWSimScene SYSTEM "UWSimScene.dtd" >

<UWSimScene>
  <oceanState>
    <windx> 1.0 </windx>
    <windy> 1.0 </windy>
    <windSpeed> 12 </windSpeed>		<!-- 0 -->
    <depth> 100 </depth>		<!-- 10000 -->
    <reflectionDamping> 0.35 </reflectionDamping> <!-- .35 -->
    <waveScale> 1e-8 </waveScale>
    <isNotChoppy> 0 </isNotChoppy>
    <choppyFactor> -2.5 </choppyFactor>
    <crestFoamHeight> 2.2 </crestFoamHeight>
    <oceanSurfaceHeight> 0 </oceanSurfaceHeight>
    <fog>
      <density> 0.05 </density>		<!-- ISR_experiments 0.05, original 0.1 -->
      <color>
      	<r>0.017</r>
      	<g>0.027</g>
      	<b>0.079</b>
      </color>
    </fog>
    <color>
    	<r>0.017</r>
    	<g>0.027</g>
    	<b>0.079</b>
    </color>
    <attenuation>
      <r>0.015</r>
      <g>0.0075 </g>
      <b> 0.005 </b>
    </attenuation>
  </oceanState>

  <simParams>
    <disableShaders> 0 </disableShaders>
    <resw> 800 </resw>
    <resh> 600 </resh>
    <offsetp>
      <x>0</x>
      <y>0</y>
      <z>0</z>
    </offsetp>
    <offsetr>
      <x> 3.14</x>
      <y> 0</y>
      <z> -1.57 </z>
    </offsetr>
    <enablePhysics> 1 </enablePhysics>
  </simParams>

  <camera>
    <freeMotion> 1 </freeMotion>	<!-- 1 -->
    <objectToTrack>girona500_RAUVI</objectToTrack>
    <fov> 60 </fov>
    <aspectRatio> 1.33 </aspectRatio>
    <near> 1 </near>
    <far> 10000 </far>			<!-- 300 -->
    <position>				      <!-- 16, 9, -7 -->
      <x> -0.3 </x>			    <!-- 0, 2, 0 -->
      <y> 0 </y>
      <z> 0 </z>
    </position>
    <lookAt>				        <!-- 5, 7, -10 -->
      <x> 0 </x>			      <!-- 0.3, 0.1, 0 -->
      <y> 0 </y>
      <z> 0 </z>
    </lookAt>
    <oculus> 0 </oculus>
    <windshield> 0 </windshield>
  </camera>

 <vehicle>
    <name>girona500_RAUVI</name>
    <file>data/scenes/g500ARM5.urdf</file>
