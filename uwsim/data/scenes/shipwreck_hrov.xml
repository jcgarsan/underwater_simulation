      <topic> /uwsim/g500/range </topic>
      <rate>10</rate>
    </RangeSensorToROSRange>
    <ROSPoseToPAT>
      <topic> /g500/pose</topic>
      <vehicleName> girona500_RAUVI </vehicleName>
    </ROSPoseToPAT>
    <PressureSensorToROS>
      <name>pressureSensor</name>
